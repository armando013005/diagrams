        <mxCell id="ZxQtin0-AZ0bA8jJVszq-6" value="&lt;b&gt;POR ÁMBITO DE APLICACIÓN&lt;/b&gt;" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
